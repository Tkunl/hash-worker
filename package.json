{
  "private": true,
  "packageManager": "pnpm@9.15.2",
  "scripts": {
    "dev": "pnpm --filter=./packages/core run dev",
    "bootstrap": "pnpm run build:core && pnpm run build:benchmark",
    "build:core": "pnpm --filter=./packages/core run build",
    "build:benchmark": "pnpm --filter=./packages/benchmark run build",
    "benchmark": "pnpm --filter=./packages/playground/benchmark run play",
    "play-node": "pnpm run build:core && pnpm --filter=./packages/playground/node-demo run play",
    "play-vue-vite": "pnpm run build:core && pnpm --filter=./packages/playground/vue-vite-demo run play",
    "play-react-vite": "pnpm run build:core && pnpm --filter=./packages/playground/react-vite-demo run play",
    "play-react-webpack": "pnpm run build:core && pnpm --filter=./packages/playground/react-webpack-demo run play",
    "test": "pnpm --filter=./packages/core run test",
    "lint": "eslint --fix",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json}'",
    "check-format": "prettier --check '**/*.{js,jsx,ts,tsx,json}'",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "commitlint": "commitlint --config commitlint.config.js -e -V",
    "sync-readme": "node scripts/syncReadme.js"
  },
  "keywords": [
    "hash-worker",
    "hash"
  ],
  "author": "Tkunl",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@jest/types": "^29.6.3",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@swc/core": "^1.10.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "browserslist": "^4.23.0",
    "chalk": "^5.4.1",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.30.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-swc3": "^0.12.1",
    "ts-jest": "^29.1.5",
    "ts-jest-mock-import-meta": "^1.2.0",
    "tsup": "^8.3.5",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5"
  }
}
