{
  "name": "hash-worker",
  "version": "1.0.1",
  "description": "hash-worker is a tool for quickly calculating file's hash",
  "author": "https://github.com/Tkunl",
  "repository": "https://github.com/Tkunl/hash-worker",
  "type": "module",
  "exports": {
    ".": {
      "browser": {
        "import": {
          "default": "./dist/browser.esm.js",
          "types": "./dist/browser.esm.d.ts"
        },
        "require": {
          "default": "./dist/global.js",
          "types": "./dist/global.d.ts"
        }
      },
      "node": {
        "import": {
          "default": "./dist/node.mjs",
          "types": "./dist/node.d.ts"
        },
        "require": {
          "default": "./dist/node.js",
          "types": "./dist/node.d.ts"
        }
      }
    },
    "./global": {
      "types": "./dist/global.d.ts",
      "require": "./dist/global.js",
      "import": "./dist/global.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "README-zh.md",
    "package.json",
    "LICENSE"
  ],
  "scripts": {
    "dev": "rollup --config rollup.config.ts --configPlugin swc3 --watch",
    "build": "pnpm rm:dist && rollup --config rollup.config.ts --configPlugin swc3",
    "test": "jest --coverage",
    "rm:dist": "rimraf ./dist"
  },
  "keywords": [
    "hash-worker",
    "hash"
  ],
  "license": "MIT",
  "dependencies": {
    "hash-wasm": "^4.12.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.2"
  }
}
